{
    "manifest_version": 2,
    "name": "PagaChecker",
    "description": "ページ診断のための拡張機能",
    "version": "1.0.2",
    "browser_action": {
        "default_icon": "./icon_38.png",
        "default_popup":"./popup.html"
    },
    "content_scripts":[
        {
            "matches":[
                "http://*/*",
                "https://*/*"
            ],
            "css":["./css/balloon.css"],
            "js":[
                "./js/jquery-3.1.1.min.js",
                "./js/jquery.balloon.min.js",
                "./js/checker-version/checker-1.0.3.js"
            ]
        }
    ]
}
